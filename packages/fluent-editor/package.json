{
  "name": "@opentiny/fluent-editor",
  "version": "3.18.0",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "license": "MIT",
  "scripts": {
    "start": "vite build && vite",
    "dev": "vite",
    "build": "vite build",
    "test": "jest",
    "format": "prettier './{src,test}/**/*.{js,ts,tsx,vue}' --write"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node verifyCommit.js"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/quill": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.5.0",
    "jest": "^26.6.3",
    "lint-staged": "^12.1.4",
    "lodash-es": "^4.17.15",
    "prettier": "^2.3.0",
    "sass": "^1.47.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vite": "^2.3.0"
  },
  "dependencies": {
    "highlight.js": "^10.2.0",
    "html2canvas": "^1.0.0-rc.7",
    "quill": "^2.0.0",
    "quill-delta": "^5.1.0"
  }
}
{
  "name": "react-component-gen",
  "version": "0.4.24",
  "description": "generate tiny-vue react components",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/react-component-gen/react-component-gen",
  "bugs": "https://github.com/react-component-gen/react-component-gen/issues",
  "repository": "react-component-gen/react-component-gen",
  "license": "MIT",
  "author": "Yanzhen Yu",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "react-component-gen": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "yarn build",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "react-component-gen",
    "commands": "./dist/commands",
    "dirname": "react-component-gen",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "change-case": "^5.4.3",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "mdast-util-from-markdown": "^2.0.0",
    "openai": "^4.29.2",
    "unist-util-visit-parents": "^6.0.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.5.4",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

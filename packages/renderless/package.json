  },
  "dependencies": {
    "xss": "1.0.11"
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": "./src/*"
  },
  "devDependencies": {
    "esno": "^0.16.3",
    "tsup": "^6.5.0"
